{
  "name": "Oracle Attribute Range Rule Template",
  "type": "ORACLE",
  "code": "ATTRRANGE",
  "template": [
    "declare",
      "l_passed boolean := true;",
    "begin",
      "if l_oper in ( 'INS', 'UPD' )",
      "then",
        "l_passed :=  :new.{attribute_column} > {range_min} && :new.{attribute_column} < {range_max};",
        "l_passed := l_passed {operand} l_passed;",
      "end if;" ,
    "end;"
  ]
}